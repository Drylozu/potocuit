{
	"name": "@potoland/core",
	"version": "0.0.1",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/cjs/index.d.ts",
	"files": [
		"dist/**",
		"src/**"
	],
	"scripts": {
		"build": "npm run build:cjs && npm run build:es6",
		"build:cjs": "tsc --outDir ./dist/cjs",
		"build:es6": "tsc --outDir ./dist/esm --module es2022",
		"clean": "rm -rf dist && rm -rf .turbo",
		"dev": "swc src -d dist -w",
		"prepublishOnly": "npm run build",
		"test:run": "cd __test__ && node ./dist/index.js",
		"test:build": "cd __test__ && nodemon --ext ts --watch ./src --exec \"swc ./src --config-file ../.swcrc -d ./dist\"",
		"test": "cd __test__ && nodemon --ext ts --watch ./src --exec \"swc ./src --config-file ../.swcrc -d ./dist && node ./dist/index.js\""
	},
	"author": "MARCROCK22",
	"license": "ISC",
	"dependencies": {
		"@biscuitland/common": "^0.0.9",
		"@biscuitland/helpers": "^3.0.12",
		"@biscuitland/rest": "^3.0.7",
		"@biscuitland/ws": "^3.0.10",
		"form-data": "^4.0.0",
		"ioredis": "^5.3.2",
		"ts-mixer": "^6.0.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.62",
		"@swc/core": "^1.3.93",
		"@types/node": "^20.6.3",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.7.2",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"nodemon": "^3.0.1",
		"ts-macros": "^2.6.0",
		"ts-patch": "^3.0.2",
		"ttsc": "^0.3.1",
		"typescript": "^5.2.2"
	},
	"optionalDependencies": {
		"tweetnacl": "^1.0.3",
		"uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.32.0"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/esm/index.d.ts",
				"default": "./dist/esm/index.js"
			},
			"require": "./dist/cjs/index.js"
		}
	},
	"homepage": "https://biscuitjs.com",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/potoland/potocuit.git"
	},
	"bugs": {
		"url": "https://github.com/potoland/potocuit"
	},
	"keywords": [
		"api",
		"discord",
		"bots",
		"typescript",
		"botdev"
	],
	"publishConfig": {
		"access": "public"
	}
}